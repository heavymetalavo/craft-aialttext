{
  "name": "heavymetalavo/craft-aialttext",
  "description": "Generate suitable alt text for CraftCMS Asset Images",
  "type": "craft-plugin",
  "keywords": [
    "craftcms",
    "craft-plugin",
    "ai",
    "alt-text",
    "accessibility"
  ],
  "support": {
    "docs": "https://github.com/heavymetalavo/craft-aialttext",
    "issues": "https://github.com/heavymetalavo/craft-aialttext/issues?state=open",
    "source": "https://github.com/heavymetalavo/craft-aialttext"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "heavymetalavo",
      "homepage": "https://github.com/heavymetalavo"
    }
  ],
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "heavymetalavo\\craftaialttext\\": "src/"
    }
  },
  "extra": {
    "handle": "ai-alt-text",
    "name": "AI Alt Text",
    "developer": "heavymetalavo",
    "documentationUrl": "https://github.com/heavymetalavo/craft-aialttext",
    "class": "heavymetalavo\\craftaialttext\\AiAltText"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
